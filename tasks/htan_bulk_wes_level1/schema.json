{
  "$id": "https://w3id.org/htan/wes/level_1",
  "$schema": "https://json-schema.org/draft-07/schema",
  "additionalProperties": {},
  "description": "Bulk Whole Exome Sequencing Level 1 - Raw files",
  "properties": {
    "ADAPTER_NAME": {
      "description": "Name of the adapter used",
      "type": "string"
    },
    "ADAPTER_SEQUENCE": {
      "description": "Adapter sequence",
      "type": "string"
    },
    "BASE_CALLER_NAME": {
      "description": "Name of the base caller",
      "type": "string"
    },
    "BASE_CALLER_VERSION": {
      "description": "Version of the base caller",
      "type": "string"
    },
    "CHECKSUM": {
      "description": "Checksum for data integrity verification",
      "type": "string"
    },
    "FILENAME": {
      "description": "Name of the file. Must end with an extension matching the FILE_FORMAT (.fastq for fastq; .fastq.gz or .fq.gz for fastq.gz)",
      "pattern": "^.+\\.(fastq|fq)(\\.gz)?$",
      "type": "string"
    },
    "FILE_FORMAT": {
      "description": "Format of the raw sequencing file (fastq or fastq.gz)",
      "pattern": "^(fastq|fastq\\.gz)$",
      "type": "string"
    },
    "FLOW_CELL_BARCODE": {
      "description": "Flow cell barcode",
      "type": "string"
    },
    "FRAGMENT_MAXIMUM_LENGTH": {
      "description": "Maximum fragment length",
      "type": "integer"
    },
    "FRAGMENT_MEAN_LENGTH": {
      "description": "Mean fragment length",
      "type": "integer"
    },
    "FRAGMENT_MINIMUM_LENGTH": {
      "description": "Minimum fragment length",
      "type": "integer"
    },
    "FRAGMENT_STANDARD_DEVIATION_LENGTH": {
      "description": "Standard deviation of fragment length",
      "type": "integer"
    },
    "GENOME_ANNOTATION_URL": {
      "description": "URL to genome annotation",
      "type": "string"
    },
    "GENOMIC_REFERENCE": {
      "description": "Genomic reference used for alignment",
      "type": "string"
    },
    "GENOMIC_REFERENCE_URL": {
      "description": "URL to genomic reference",
      "type": "string"
    },
    "HTAN_DATA_FILE_ID": {
      "description": "HTAN Data File ID (Primary Key)",
      "pattern": "^(?=.{1,50}$)(HTA2[0-2][0-9])_(0000|EXT[0-9]{1,18}|[0-9]{1,21})_(D[0-9]{1,20})$",
      "type": "string"
    },
    "HTAN_PARENT_ID": {
      "description": "HTAN Parent ID - Foreign Key to parent entity (B for Biospecimen, D for data file). Must have B or D suffix. Supports HTA200-229 for phase 2.",
      "pattern": "^(?=.{1,50}$)(HTA2[0-2][0-9])_(0000|EXT[0-9]{1,18}|[0-9]{1,21})_([BD][0-9]{1,20})$",
      "type": "string"
    },
    "LANE_NUMBER": {
      "description": "Lane number",
      "type": "integer"
    },
    "LIBRARY_LAYOUT": {
      "description": "",
      "enum": [
        "Paired-end",
        "Single-end"
      ],
      "title": "LibraryLayoutEnum",
      "type": "string"
    },
    "LIBRARY_PREPARATION_DAYS_FROM_INDEX": {
      "description": "Days from index for library preparation",
      "type": "integer"
    },
    "LIBRARY_PREPARATION_KIT_NAME": {
      "description": "Name of the library preparation kit",
      "type": "string"
    },
    "LIBRARY_PREPARATION_KIT_VENDOR": {
      "description": "Vendor of the library preparation kit",
      "type": "string"
    },
    "LIBRARY_PREPARATION_KIT_VERSION": {
      "description": "Version of the library preparation kit",
      "type": "string"
    },
    "LIBRARY_SELECTION_METHOD": {
      "description": "",
      "enum": [
        "Hybrid Selection",
        "PCR",
        "RANDOM",
        "other"
      ],
      "title": "LibrarySelectionMethodEnum",
      "type": "string"
    },
    "MULTIPLEX_BARCODE": {
      "description": "Multiplex barcode",
      "type": "string"
    },
    "PROTOCOL_LINK": {
      "description": "Link to sequencing protocol",
      "type": "string"
    },
    "READ_INDICATOR": {
      "description": "Read indicator",
      "type": "string"
    },
    "READ_LENGTH": {
      "description": "Read length in base pairs",
      "type": "integer"
    },
    "SEQUENCING_BATCH_ID": {
      "description": "Sequencing batch identifier",
      "type": "string"
    },
    "SEQUENCING_PLATFORM": {
      "description": "",
      "enum": [
        "ABI_SOLID",
        "BGISEQ",
        "CAPILLARY",
        "COMPLETE_GENOMICS",
        "HELICOS",
        "ILLUMINA",
        "ION_TORRENT",
        "LS454",
        "OXFORD_NANOPORE",
        "PACBIO_SMRT"
      ],
      "title": "SequencingPlatformEnum",
      "type": "string"
    },
    "SIZE_SELECTION_RANGE": {
      "description": "Size selection range",
      "type": "string"
    },
    "TARGET_CAPTURE_KIT": {
      "description": "Target capture kit used",
      "type": "string"
    },
    "TARGET_DEPTH": {
      "description": "Target sequencing depth",
      "type": "integer"
    },
    "TECHNICAL_REPLICATE_GROUP": {
      "description": "Technical replicate group identifier",
      "type": "string"
    },
    "TO_TRIM_ADAPTER_SEQUENCE": {
      "description": "Whether to trim adapter sequence",
      "type": "boolean"
    },
    "WORKFLOW_LINK": {
      "description": "Link to workflow or command. DockStore.org recommended",
      "type": "string"
    },
    "WORKFLOW_VERSION": {
      "description": "Major version of the workflow",
      "type": "string"
    }
  },
  "required": [
    "FILE_FORMAT",
    "FILENAME",
    "LIBRARY_SELECTION_METHOD",
    "READ_LENGTH",
    "LIBRARY_LAYOUT",
    "SEQUENCING_PLATFORM",
    "WORKFLOW_VERSION",
    "GENOMIC_REFERENCE",
    "HTAN_DATA_FILE_ID",
    "HTAN_PARENT_ID"
  ],
  "title": "Level1",
  "type": "object"
}